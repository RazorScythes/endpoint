{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/tags.js",
            "schedule": "1 5 * * *"
        }
    ]
}